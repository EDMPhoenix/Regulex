{
  "name": "regulex",
  "version": "1.0.0",
  "description": "Regular Expression Excited!",
  "author": {
    "name": "yumenotame"
  },
  "main": "./build/src/index",
  "files": [
    "build",
    "src",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EdmTechPhoenix/Regulex.git"
  },
  "keywords": [
    "Regular Expression",
    "RegExp",
    "Regex",
    "XRegExp",
    "PCRE",
    "parser",
    "Regulex"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.202",
    "immer": "^10.0.3",
    "lodash": "^4.17.21"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/benchmark": "^2.1.5",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.3",
    "acorn": "^8.11.2",
    "benchmark": "^2.1.4",
    "chai": "^4.3.10",
    "fast-check": "^1.16.0",
    "mocha": "^10.2.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "unicode-12.0.0": "^0.8.0",
    "utility-types": "^3.10.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "mocha --inline-diffs --require ts-node/register --recursive test/**/*Spec.ts",
    "testit": "mocha --inline-diffs --require ts-node/register",
    "format": "prettier --write \"{src, test}/**/*.{js,ts,css}\" \"./*.{js,ts,css}\"",
    "buildUnicode": "ts-node src/tools/buildUnicode.ts && npm run format && npm run build",
    "prepare": "npm run build"
  }
}
