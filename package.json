{
  "name": "regulex",
  "version": "1.0.0",
  "description": "Regular Expression Excited!",
  "author": {
    "name": "yumenotame"
  },
  "main": "./build/src/index",
  "files": [
    "build",
    "src",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/EdmTechPhoenix/Regulex.git"
  },
  "keywords": [
    "Regular Expression",
    "RegExp",
    "Regex",
    "XRegExp",
    "PCRE",
    "parser",
    "Regulex"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.3",
    "@types/power-assert": "^1.5.12",
    "assert": "^2.1.0",
    "espower-typescript": "^10.0.1",
    "fast-check": "^3.14.0",
    "mocha": "^10.2.0",
    "power-assert": "^1.6.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "utility-types": "^3.10.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "mocha --recursive --require espower-typescript/guess test/*Spec.ts",
    "testit": "mocha --require espower-typescript/guess",
    "format": "prettier --write \"{src, test}/**/*.{js,ts,css}\" \"./*.{js,ts,css}\"",
    "prepare": "npm run build"
  }
}
